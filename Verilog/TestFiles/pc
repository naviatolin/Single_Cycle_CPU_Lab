    .scope S_0x7f86bbd01b70;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x7f86bbd1abe0_0;
    %nor/r;
    %store/vec4 v0x7f86bbd1abe0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7f86bbd01b70;
T_3 ;
    %wait E_0x7f86bbd08490;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x7f86bbd1aa80_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7f86bbd1ab10_0, 0, 32;
