#! /usr/local/Cellar/icarus-verilog/10.3/bin/vvp
:ivl_version "10.3 (stable)" "(v10_3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x7fb001403e60 .scope module, "PCtest" "PCtest" 2 3;
 .timescale 0 0;
v0x7fb00141cba0_0 .net "PC", 31 0, v0x7fb00140bcf0_0;  1 drivers
v0x7fb00141cc50_0 .var "PC_last", 31 0;
v0x7fb00141cce0_0 .var "alternative_PC", 31 0;
v0x7fb00141cdb0_0 .var "clk", 0 0;
v0x7fb00141ce60_0 .var "use_alternative_PC", 0 0;
E_0x7fb00140a000 .event negedge, v0x7fb00141c9a0_0;
S_0x7fb001402370 .scope module, "dut" "PC" 2 13, 3 1 0, S_0x7fb001403e60;
 .timescale 0 0;
    .port_info 0 /OUTPUT 32 "PC"
    .port_info 1 /INPUT 32 "PC_last"
    .port_info 2 /INPUT 32 "alternative_PC"
    .port_info 3 /INPUT 1 "use_alternative_PC"
    .port_info 4 /INPUT 1 "clk"
P_0x7fb001402690 .param/l "increment" 0 3 5, C4<00000000000000000000000000000100>;
P_0x7fb0014026d0 .param/l "width" 0 3 4, +C4<00000000000000000000000000100000>;
v0x7fb00140bcf0_0 .var "PC", 31 0;
v0x7fb00141c830_0 .net "PC_last", 31 0, v0x7fb00141cc50_0;  1 drivers
v0x7fb00141c8e0_0 .net "alternative_PC", 31 0, v0x7fb00141cce0_0;  1 drivers
v0x7fb00141c9a0_0 .net "clk", 0 0, v0x7fb00141cdb0_0;  1 drivers
v0x7fb00141ca40_0 .net "use_alternative_PC", 0 0, v0x7fb00141ce60_0;  1 drivers
E_0x7fb00140b310 .event posedge, v0x7fb00141c9a0_0;
    .scope S_0x7fb001402370;
T_0 ;
    %wait E_0x7fb00140b310;
    %load/vec4 v0x7fb00141ca40_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x7fb00141c8e0_0;
    %store/vec4 v0x7fb00140bcf0_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fb00141c830_0;
    %addi 4, 0, 32;
    %store/vec4 v0x7fb00140bcf0_0, 0, 32;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fb001403e60;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fb00141cdb0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x7fb001403e60;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x7fb00141cdb0_0;
    %nor/r;
    %store/vec4 v0x7fb00141cdb0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x7fb001403e60;
T_3 ;
    %wait E_0x7fb00140a000;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x7fb00141cc50_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fb00141cce0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fb00141ce60_0, 0, 1;
    %delay 1, 0;
    %wait E_0x7fb00140b310;
    %load/vec4 v0x7fb00141cba0_0;
    %load/vec4 v0x7fb00141cce0_0;
    %cmp/ne;
    %jmp/0xz  T_3.0, 4;
    %vpi_call 2 24 "$display", "Fail at using alternative PC to reset the PC" {0 0 0};
    %jmp T_3.1;
T_3.0 ;
    %vpi_call 2 25 "$display", "Reset PC Works!" {0 0 0};
T_3.1 ;
    %wait E_0x7fb00140a000;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fb00141cc50_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x7fb00141cce0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fb00141ce60_0, 0, 1;
    %delay 1, 0;
    %wait E_0x7fb00140b310;
    %load/vec4 v0x7fb00141cba0_0;
    %cmpi/ne 4, 0, 32;
    %jmp/0xz  T_3.2, 4;
    %vpi_call 2 37 "$display", "Fail at adding 4 to PC" {0 0 0};
    %jmp T_3.3;
T_3.2 ;
    %vpi_call 2 38 "$display", "Adding 4 to PC works!" {0 0 0};
T_3.3 ;
    %wait E_0x7fb00140a000;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x7fb00141cc50_0, 0, 32;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x7fb00141cce0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fb00141ce60_0, 0, 1;
    %delay 1, 0;
    %wait E_0x7fb00140b310;
    %load/vec4 v0x7fb00141cba0_0;
    %load/vec4 v0x7fb00141cce0_0;
    %cmp/ne;
    %jmp/0xz  T_3.4, 4;
    %vpi_call 2 50 "$display", "Fail at using alternative PC to reset the PC" {0 0 0};
    %jmp T_3.5;
T_3.4 ;
    %vpi_call 2 51 "$display", "Alternative PC Works!" {0 0 0};
T_3.5 ;
    %vpi_call 2 54 "$finish" {0 0 0};
    %end;
    .thread T_3;
