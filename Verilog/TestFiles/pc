    .scope S_0x7f86bbd00080;
T_0 ;
    %wait E_0x7f86bbd08d70;
    %load/vec4 v0x7f86bbd1a8b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x7f86bbd1a720_0;
    %store/vec4 v0x7f86bbd1a670_0, 0, 32;
    %jmp T_0.1;
T_0.0 ;
